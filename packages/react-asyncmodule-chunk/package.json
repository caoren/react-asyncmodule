{
  "name": "react-asyncmodule-chunk",
  "version": "1.2.0-beta.4",
  "description": "react asyncmodule chunk",
  "author": "caoren",
  "sideEffects": false,
  "keywords": [
    "react",
    "asyncmodule",
    "provider"
  ],
  "bugs": {
    "url": "https://github.com/caoren/react-asyncmodule/issues"
  },
  "homepage": "https://github.com/caoren/react-asyncmodule",
  "repository": {
    "type": "git",
    "url": "https://github.com/caoren/react-asyncmodule.git"
  },
  "scripts": {
    "js:commonjs": "cross-env BABEL_ENV=commonjs babel src/ --out-dir dist/",
    "js:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "test": "jest",
    "build": "npm run test && npm run js:commonjs && npm run js:es"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^23.5.0",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "peerDependencies": {
    "prop-types": ">=15.3.0",
    "react": ">=15.3.0",
    "react-dom": ">=15.3.0"
  },
  "dependencies": {
    "create-react-context": ">=0.3.0",
    "hoist-non-react-statics": "^3.3.0"
  },
  "engines": {
    "node": ">= 6.2.0",
    "npm": ">= 3.0.0"
  },
  "jsnext:main": "es/index.js",
  "main": "dist/index.js",
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "testMatch": [
      "<rootDir>/test/*.js?(x)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "gitHead": "e704bf27d8842530793674cac7c39e6dfcf83d89"
}
