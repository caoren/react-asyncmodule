{
  "name": "react-asyncmodule",
  "version": "1.0.0-alpha.3",
  "description": "react async load component",
  "author": "caoren",
  "keywords": [
    "react",
    "async",
    "module"
  ],
  "bugs": {
    "url": "https://github.com/caoren/react-asyncmodule/issues"
  },
  "homepage": "https://github.com/caoren/react-asyncmodule",
  "repository": {
    "type": "git",
    "url": "https://github.com/caoren/react-asyncmodule.git"
  },
  "scripts": {
    "dev": "webpack-dev-server --port 7909 --content-base demo/ --config demo/webpack.config.js --progress --hot --inline",
    "js:commonjs": "cross-env BABEL_ENV=commonjs babel src/ --out-dir dist/",
    "js:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "test": "jest",
    "build": "npm run test && npm run js:commonjs && npm run js:es"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-asyncmodule-import": "^0.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "jest": "^22.2.2",
    "less": "^3.8.1",
    "less-loader": "^2.2.3",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-inline-urls": "^1.2.0",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-hot-loader": "^4.8.5",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "url-loader": "^1.1.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.11.2",
    "yargs": "^9.0.1"
  },
  "peerDependencies": {
    "prop-types": ">=15.3.0",
    "react": ">=15.3.0",
    "react-dom": ">=15.3.0"
  },
  "engines": {
    "node": ">= 6.2.0",
    "npm": ">= 3.0.0"
  },
  "jsnext:main": "es/index.js",
  "main": "dist/index.js",
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "testMatch": [
      "<rootDir>/test/*.js?(x)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "create-react-context": "^0.2.3",
    "hoist-non-react-statics": "^3.3.0"
  }
}
